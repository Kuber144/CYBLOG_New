{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "frontend", "use": "@vercel/static-build" },
    { "src": "realtime-code-editor", "use": "@vercel/node" },
    { "src": "./index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/frontend/(.*)", "dest": "frontend/$1" },
    { "src": "^/realtime-code-editor/(.*)", "dest": "realtime-code-editor/$1" },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "index.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
